@charset "utf-8";

.productiontimer{
    position: absolute;
    top: 0;
    z-index: 2;
}
.arrow_left, .arrow_right {
	width: 32px;
	height: 32px;
	position: relative;
	top: 50%;
	margin-top: -16px;
	z-index: 110;
	margin-right: 5px;
	margin-left: 5px;
	display: block;
	
}
.arrow_left {
	float: left;
}
.arrow_right {
	float: right;
}
.arrow_left, .arrow_right {
    cursor: pointer;
}
/* STYLES FOR PRODUCTION TIMER BLOCK */
.production_timer_block_position {
	margin-left: auto;
	margin-right: auto;
	width: 100%;
    height: 427px;
}
.production_timer_block_container {
	width: 100%;
	position: relative;
	overflow: hidden;
}
.overlay {
	height: 427px;
	width: 100%;
	padding-left: 5px;
}
.timer_header
{
color: #909090;    
font-family: "Arial", "Helvetica", sans-serif;
font-size: 20px;     
}
.break_duration_timer {
    margin-top: 20px;
}
.time_to_return_timer  {
    margin-top: 10px;
}
.system_time_cd {
    margin-top: 25px;   
}
.production_timer_block_table {
    border-collapse: separate;
    border-spacing: 0px;
    width: 100%;
 }
.production_timer_block_table td{
    padding:0px; 
 }
 .five_seconds_warning {
     display: block;
     float: left;
 }
 .five_seconds_lamp {
    background-color: #595959;
    float: left;
    font-family: "Arial", "Helvetica", sans-serif;
    width: 36px;
    height: 36px;
    border-radius: 36px;
    margin-top: 5px;
    margin-right: 15px;
    margin-left: 16px;
    position: relative;
    vertical-align: top;
 }
 .empty_timer {
     height: 90px;
 }
 .active {
     background-color: #8c2222;
 }
 .production_timer_data_grid 
 {
     width:99%;
	height: 480px;
    overflow-y: auto;
	-webkit-overflow-scrolling: touch;
	-webkit-box-flex: 1;
	margin-left: auto;
	margin-right: auto;
 }
 .status_cell {
     width: 16px;
     height: 16px;
     margin-top: 5px;
     margin-left: auto;
     margin-right: auto;
     background-position: center;
    background-repeat: no-repeat;
 }
